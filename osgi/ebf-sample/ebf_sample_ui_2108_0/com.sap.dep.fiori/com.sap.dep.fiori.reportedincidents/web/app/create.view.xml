				<VBox
                    fitContainer="true"
                    renderType="Bare">
                    <HBox renderType="Bare">
					     <Button id="initialsButton2" text="{parts: [ 'INJURED_PERSON>/PERSON_DETAILS/FIRST_NAME', 'INJURED_PERSON>/PERSON_DETAILS/LAST_NAME'], 
                                                                   formatter: '.formatters.nameInitials'}" class="sapUiSmallMarginEnd"/>
					    <VBox>
                           
                            <Label text="{INJURED_PERSON>/PERSON_DETAILS/FIRST_NAME} {INJURED_PERSON>/PERSON_DETAILS/LAST_NAME}" id="nameField" class="sapUiTinyMarginTop"/>                    
					
							 <HBox renderType="Bare">
								<Label
									text="{i18n>ObjectPageContentHeader.incidentID}:"
									class="sapUiTinyMarginEnd" />
								<Label text="{INJURED_PERSON>/ID_LOCAL}"/>
							</HBox>
						</VBox>
					</HBox>
                   
                </VBox>	
            </u:headerContent>

            <u:sections>
                <u:ObjectPageSection title="{i18n>ObjectPageSection.injuredPerson}" id="page" >
