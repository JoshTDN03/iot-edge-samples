                                <f:Form id="form">

                                    <f:layout>
                                        <f:ResponsiveGridLayout
                                            hSpacing="2"
                                            /> 
                                    </f:layout>
                                    <f:toolbar>
										<Toolbar class="sapUiTinyMarginBottom">
											<Title text="{i18n>FormContainer.reportedincidents}" />										
											<ToolbarSpacer />
											<HBox visible="{= ${INJURED_PERSON>/IS_SUBMITTED} === ''}">
												<Button
													text="{i18nGlobal>General.edit}"
													id="personEditButton"
													press="onPersonEdit"
													visible="{= !${state>/editing}}" />
												<Button
													id="personSaveButton"
													text="{i18nGlobal>General.save}"
													press="onPersonSave"
													visible="{state>/editing}" />
												<Button
													text="{i18nGlobal>General.cancel}"
													press="onPersonCancel"
													visible="{state>/editing}" />
											</HBox>
										</Toolbar>
									</f:toolbar>
                                    <f:formContainers>
                                        <f:FormContainer>
