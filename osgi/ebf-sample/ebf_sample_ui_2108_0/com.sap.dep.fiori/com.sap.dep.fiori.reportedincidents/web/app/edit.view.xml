<mvc:View
    controllerName="dep.fiori.reportedincidents.app.edit"
    class="dep-fiori"
    displayBlock="true"
    height="100%"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:c="sap.ui.core"
    xmlns:u="sap.uxap"
    xmlns="sap.m">

    <Page
        showHeader="false">
        <u:ObjectPageLayout alwaysShowContentHeader="true" useIconTabBar="true">

            <u:headerTitle>
                <u:ObjectPageHeader objectTitle="{i18n>Page.incidentTitle}: {INCIDENT_MODEL>/ID_LOCAL}">
                    <!-- <u:actions>
                        <u:ObjectPageHeaderActionButton text="{i18n>Form.authorLabel}" enabled="false" hideText="false" id="reportedByButton"/>
                        <u:ObjectPageHeaderActionButton enabled="false" id="reportedByIcon"
                                                        icon="{INCIDENT_MODEL>/IMAGE}"/>
                    </u:actions> -->
					<u:actions>
					<!--	 <Button id="submit-button-top" type="Emphasized" text="{i18n>Footer.submit}" press="onSubmitPress"/>	-->							
                    </u:actions>
                </u:ObjectPageHeader>
            </u:headerTitle>

            <u:headerContent>
               <!--  <l:HorizontalLayout width="100%">
                   <core:Icon src="{path: 'INCIDENT_MODEL>/STATUS_KEY', formatter: '.formatters.iconFormatter'}"
                               size="1.5rem"
                               color="{path: 'INCIDENT_MODEL>/STATUS_KEY', formatter: '.formatters.iconColorFormatter'}"
                               class="sapUiSmallMarginEnd"
                               id="statusIcon"/>
                    <Label text="{INCIDENT_MODEL>/STATUS}" id="statusText"/> 
                </l:HorizontalLayout>-->
				 <VBox
                    fitContainer="true"
                    renderType="Bare">
                    <HBox renderType="Bare">
                        <Label
                            text="{i18n>ObjectPageContentHeader.status}:"
                            class="sapUiTinyMarginEnd" />
                        <Label text="{INCIDENT_MODEL>/STATUS}" id="incidentStatusText"/>
                    </HBox>
					<HBox renderType="Bare" visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === 'X'}">
                        <Label
                            text="{i18n>ObjectPageContentHeader.ehsID}:"
                            class="sapUiTinyMarginEnd" />
                        <Label text="{INCIDENT_MODEL>/ID}" id="incidentEHSIDText"/>
                    </HBox>
                    <HBox renderType="Bare">
                        <Label
                            text="{i18n>ObjectPageContentHeader.updatedBy}:"
                            class="sapUiTinyMarginEnd" />
                        <Label text="{INCIDENT_MODEL>/LAST_UPDATED_BY}"  id="updatedByText"/>
                    </HBox>
					<HBox renderType="Bare">
                        <Label
                            text="{i18n>ObjectPageContentHeader.updatedOn}:"
                            class="sapUiTinyMarginEnd" />
                        <Label text="{INCIDENT_MODEL>/LAST_UPDATED_ON}"  id="updatedOnText"/>
                    </HBox>
                </VBox>
            </u:headerContent>

            <u:sections>
                <u:ObjectPageSection title="{i18n>ObjectPageSection.basicInfo}" id="page" >
                    <u:subSections>
                        <u:ObjectPageSubSection>
                            <u:blocks>
                                <f:Form id="form" class="sapUiSmallMarginTop">
                                    <f:layout>
                                        <f:ResponsiveGridLayout
                                            labelSpanL="2"
                                            />
                                    </f:layout>
									<f:toolbar>
										<Toolbar class="sapUiTinyMarginBottom"  visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}">
											<Title text="{i18n>FormContainer.reportedincidents}" />										
											<ToolbarSpacer />
											<Button
												text="{i18nGlobal>General.edit}"
												id="incidentEditButton"
												press="onEdit"
												visible="{= !${state>/editing}}" />
											<Button
												id="save-button"
												text="{i18nGlobal>General.save}"
												press="onSave"
												visible="{state>/editing}" />
											<Button
												text="{i18nGlobal>General.cancel}"
												press="onCancel"
												visible="{state>/editing}" />
										</Toolbar>
									</f:toolbar>
                                    <f:formContainers>
                                        <f:FormContainer>
                                            <f:formElements>
												<f:FormElement>
                                                        <Label text="{i18n>Form.incidentTitleWhat}" design="Bold">
															<layoutData>
                                                                <l:GridData
                                                                    span="L12 M12 S12">
                                                                </l:GridData>
                                                            </layoutData>
														</Label>	                                                 
                                                </f:FormElement>												
                                                <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.incidentTileLabel}" required="true"/>
                                                    </f:label>

                                                    <f:fields>
                                                        <Input
                                                            change="validate"
                                                            id="incidentTitle"
                                                            value="{INCIDENT_MODEL>/TITLE}"
															maxLength="80"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </Input>
                                                    </f:fields>
                                                </f:FormElement>
                                                <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.descriptionLabel}" required="false"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <TextArea
                                                            id="descriptionOfEvents"
                                                            value="{INCIDENT_MODEL>/ELTXT}"
                                                            change="validate"
                                                            rows="4"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </TextArea>
                                                    </f:fields>
                                                </f:FormElement>
												 <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.immediateActions}" required="false"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <TextArea
                                                            id="actionnsOfEvents"
                                                            value="{INCIDENT_MODEL>/ALTXT}"
                                                            change="validate"
                                                            rows="4"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </TextArea>
                                                    </f:fields>
                                                </f:FormElement>

												<f:FormElement>
                                                        <Label text="{i18n>Form.incidentTitleWhen}" design="Bold">
															<layoutData>
                                                                <l:GridData
                                                                    span="L12 M12 S12">
                                                                </l:GridData>
                                                            </layoutData>
														</Label>	                                                   
                                                </f:FormElement>
												                                             
<!--                                                 <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18nGlobal>General.priority}" required="true"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <Select
                                                            id="incident-reporting-priority"
                                                            items="{path: '/'}"
                                                            change="validate"
                                                            selectedKey="{INCIDENT_MODEL>/PRIORITY_KEY}">
                                                            <c:Item key="{PRIORITY_ID}" text="{PRIORITY_TYPE}" />
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L2 M2 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </Select>

                                                    </f:fields>
                                                </f:FormElement> -->

                                                <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.dateLabel}" required="true"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <DatePicker
                                                            id="datePicker"
                                                            placeholder="{i18nGlobal>General.enterDate} ..."
                                                            valueFormat="yyyyMMdd"
                                                            value="{INCIDENT_MODEL>/START_DATE}"
                                                            change="validate"
                                                            class="sapUiSmallMarginBottom"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L2 M2 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </DatePicker>
                                                    </f:fields>
                                                    <f:fields>
                                                        <TimePicker
                                                            id="timePicker"
                                                            value="{INCIDENT_MODEL>/START_TIME}"
                                                            valueFormat="HHmm"
                                                            displayFormat="HH:mm"
                                                            class="sapUiLargeMarginEnd"
                                                            change="validate"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L1 M1 S6">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </TimePicker>

                                                        <Select
                                                            id="incident-reporting-timezone"
                                                            showSecondaryValues="true"
                                                            selectedKey="{INCIDENT_MODEL>/START_TIME_ZONE}"
                                                            items="{path: '/'}"
                                                            change="validate"
															enabled="{state>/editing}"
															>
                                                            <c:ListItem key="{START_TIME_ZONE}" text="{START_TIME_ZONE}" enabled="{state>/editing}" additionalText="{TIMEZONE_DESCRIPTION}"/>
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L2 M4 S6">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </Select>
                                                    </f:fields>
                                                </f:FormElement>
												
												 <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.unknownTimeLabel}" required="false"/>
                                                    </f:label>
                                                    <f:fields>
														 <Checkbox
														selected="{=${INCIDENT_MODEL>/TIME_NOT_DET_IND} === 'X'}"
														select="onUnknownTimeSelect"
														enabled="{state>/editing}"/>
                                                    </f:fields>
                                                </f:FormElement>
												
												<f:FormElement>
                                                        <Label text="{i18n>Form.incidentTitleWhere}" design="Bold">
															<layoutData>
                                                                <l:GridData
                                                                    span="L12 M12 S12">
                                                                </l:GridData>
                                                            </layoutData>
														</Label>	                                                 
                                                </f:FormElement>
												<f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.plantIDLabel}" required="false"/>
                                                    </f:label>

                                                    <f:fields>
                                                        <Input
                                                            change="validate"
                                                            id="incidentPlantID"
                                                            value="{INCIDENT_MODEL>/PLANT_ID}"
															enabled="false">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </Input>
                                                    </f:fields>
                                                </f:FormElement>
												
												  <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Form.locationLabel}" required="false"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <Input
                                                            id="incident-reporting-location"
                                                            showValueHelp="true"
                                                            change="validate"
                                                            value="{INCIDENT_MODEL>/PLANT_LOC_DESCRIPTION}"
                                                            valueHelpOnly="true"
                                                            valueHelpRequest="accidentLocationPress"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </Input>
														<Input
															type="Text"
															value="{/PLANT_LOC_CODE}"
															enabled="false"
															editable="false"
															showValueHelp="false"
															visible="false"
															>
														</Input>
                                                    </f:fields>
                                                </f:FormElement>
																							
												<f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Location.descriptionLabel}" required="false"/>
                                                    </f:label>
                                                    <f:fields>
                                                        <TextArea
                                                            id="locationDescription"
                                                            value="{INCIDENT_MODEL>/DESCRIPTION}"
                                                            change="validate"
                                                            rows="4"
															editable="{state>/editing}">
                                                            <layoutData>
                                                                <l:GridData
                                                                    span="L5 M5 S12">
                                                                </l:GridData>
                                                            </layoutData>
                                                        </TextArea>
                                                    </f:fields>
                                                </f:FormElement>
												
												 <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Location.locationClassificationLabel}" required="false"/>
                                                    </f:label>

                                                    <f:fields>
                                                       <Input 
														id="locationClassification"
														showValueHelp="true"
														value="{INCIDENT_MODEL>/PLANT_LOC_CL_DESCRIPTION}"
														valueHelpOnly="true"
														valueHelpRequest="accidentLocationClassificationPress"
														editable="{state>/editing}">
														<layoutData>
															<l:GridData
																span="L5 M5 S12">
															</l:GridData>
														</layoutData>
													</Input>
													<Input
														type="Text"
														value="{/PLANT_LOC_CL_CODE}"
														enabled="false"
														editable="false"
														showValueHelp="false"
														visible="false"
														>
													</Input>
                                                    </f:fields>
                                                </f:FormElement>
												
												 <f:FormElement>
                                                    <f:label>
                                                        <Label text="{i18n>Location.organizationalUnitLabel}" required="false"/>
                                                    </f:label>

                                                    <f:fields>
                                                        <Input
															showValueHelp="true"
															change="validate"
															id="locationOrgUnitID"                                    
															value="{INCIDENT_MODEL>/ORG_UNIT_DESCRIPTION}"
															valueHelpOnly="true"
															valueHelpRequest="orgUnitPress"
															editable="{state>/editing}"
															>
															<layoutData>
																<l:GridData
																	span="L5 M5 S12">
																</l:GridData>
															</layoutData>
														</Input>
														<Input
															type="Text"
															value="{/ORG_UNIT}"
															enabled="false"
															editable="false"
															showValueHelp="false"
															visible="false"
															>
														</Input>
                                                    </f:fields>
                                                </f:FormElement>
												
                                            </f:formElements>
                                        </f:FormContainer>
                                    </f:formContainers>
                                </f:Form>

                            </u:blocks>
                        </u:ObjectPageSubSection>
                    </u:subSections>
                </u:ObjectPageSection>



                <u:ObjectPageSection title="{i18n>ObjectPageSection.people}" id="page2" >
                    <u:subSections>
                        <u:ObjectPageSubSection>
                            <u:blocks>
                                <ScrollContainer
                                    vertical="true"
                                    height="100%"
                                    width="100%">
                                    <Table
                                        noDataText="{i18n>InjuredPeopleList.empty}"
                                        items="{INJURED_PEOPLE>/}"
                                        growing="true"
                                        includeItemInSelection="true"
                                        growingScrollToLoad="true">
                                        <headerToolbar>
                                            <Toolbar>
                                                <Title
                                                    id="tableTitle"
                                                    text="{ path: 'INJURED_PEOPLE>/length', formatter: '.formatters.listTitle' }"
                                                    level="H2"/>
                                                <ToolbarSpacer></ToolbarSpacer>
                                                <Button
                                                    id="peopleAddButton"
                                                    tooltip="{i18n>Tooltip.addPersonButton}"
                                                    icon="sap-icon://add"
													visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
                                                    press="createPeople" />
                                            </Toolbar>
                                        </headerToolbar>

                                        <columns>
                                            <Column
                                                demandPopin="true"
                                                vAlign="Middle">
												<Text text="{i18n>InjuredPeopleList.name}" />
                                            </Column>
                                            <Column
                                                demandPopin="true"
                                                vAlign="Middle">
												<Text text="{i18n>InjuredPeopleList.type}" />
                                            </Column>
                                            <Column
                                                demandPopin="true"
                                                hAlign="End"
                                                vAlign="Middle">
                                            </Column>
                                        </columns>
                                       <!-- <items>
                                            <ColumnListItem type="Active" press="navToAddInjuredPerson">
                                                <cells>
                                                    <Button id="initialsButton" text="{parts: [ 'INJURED_PEOPLE>PERSON_DETAILS/FIRST_NAME', 'INJURED_PEOPLE>PERSON_DETAILS/LAST_NAME'],
                                                                   formatter: '.formatters.nameInitials'}" class="sapUiSmallMarginBottom"/>
                                                    <Label text="{INJURED_PEOPLE>PERSON_DETAILS/FIRST_NAME} {INJURED_PEOPLE>PERSON_DETAILS/LAST_NAME}" id="nameField"/>
                                                    <c:Icon src="sap-icon://navigation-right-arrow"/>
                                                </cells>
                                            </ColumnListItem>
                                        </items>-->
										 <ColumnListItem vAlign="Middle" type="Active"  press="navToAddInjuredPerson">
											<Label
												text="{INJURED_PEOPLE>FIRST_NAME} {INJURED_PEOPLE>LAST_NAME}" id="initialsButton"
												tooltip="{INJURED_PEOPLE>FIRST_NAME} {INJURED_PEOPLE>LAST_NAME}"/>
											
											<Label text="{INJURED_PEOPLE>INVOLVE_TYPE_DESC}" id="typeField"/>
											
											<Button
												icon="sap-icon://sys-cancel"
												type="Transparent"
												visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
												press="removePersonPress" />
										</ColumnListItem>
                                    </Table>
                                </ScrollContainer>

                            </u:blocks>
                        </u:ObjectPageSubSection>
                    </u:subSections>
                </u:ObjectPageSection>

				<u:ObjectPageSection title="{i18n>ObjectPageSection.equipments}" id="page3" >
                    <u:subSections>
                        <u:ObjectPageSubSection>
                            <u:blocks>
                                <ScrollContainer
                                    vertical="true"
                                    height="100%"
                                    width="100%">
                                    <Table
                                        noDataText="{i18n>EquipmentsList.empty}"
                                        items="{INVOLVED_EQUIPMENT>/}"
                                        growing="true"
                                        includeItemInSelection="true"
                                        growingScrollToLoad="true">
                                        <headerToolbar>
                                            <Toolbar>
                                                <Title
                                                    id="equipmentsTableTitle"
                                                    text="{ path: 'INVOLVED_EQUIPMENT>/length', formatter: '.formatters.equipmentsListTitle' }"
                                                    level="H2"/>
                                                <ToolbarSpacer></ToolbarSpacer>
                                                <Button
                                                    id="equipmentAddButton"
                                                    tooltip="{i18n>Tooltip.addEquipmentButton}"
                                                    icon="sap-icon://add"
													visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
                                                    press="createEquipment" />
                                            </Toolbar>
                                        </headerToolbar>

                                        <columns>
                                            <Column
                                                demandPopin="true"
                                                vAlign="Middle">
												<Text text="{i18n>EquipmentsList.description}" />
                                            </Column>
                                            <Column
                                                demandPopin="true"
                                                hAlign="End"
                                                vAlign="Middle">
                                            </Column>
                                        </columns>
										 <ColumnListItem vAlign="Middle" type="Active"  press="navToAddEquipment">											
											<Label text="{INVOLVED_EQUIPMENT>EQUIPMENT_DESC}" id="equipmentDescription"/>												
											<Button
												icon="sap-icon://sys-cancel"
												type="Transparent"
												visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
												press="removeEquipmentPress" />
										</ColumnListItem>
                                    </Table>
                                </ScrollContainer>

                            </u:blocks>
                        </u:ObjectPageSubSection>
                    </u:subSections>
                </u:ObjectPageSection>
  
				<u:ObjectPageSection title="{i18n>ObjectPageSection.vehicle}" id="page4" >
                    <u:subSections>
                        <u:ObjectPageSubSection>
                            <u:blocks>
                                <ScrollContainer
                                    vertical="true"
                                    height="100%"
                                    width="100%">
                                    <Table
                                        noDataText="{i18n>VehicleList.empty}"
                                        items="{INVOLVED_VEHICLE>/}"
                                        growing="true"
                                        includeItemInSelection="true"
                                        growingScrollToLoad="true">
                                        <headerToolbar>
                                            <Toolbar>
                                                <Title
                                                    id="vehicleTableTitle"
                                                    text="{ path: 'INVOLVED_VEHICLE>/length', formatter: '.formatters.vehicleListTitle' }"
                                                    level="H2"/>
                                                <ToolbarSpacer></ToolbarSpacer>
                                                <Button
                                                    id="vehicleAddButton"
                                                    icon="sap-icon://add"
													visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
                                                    press="createVehicle" />
                                            </Toolbar>
                                        </headerToolbar>

                                        <columns>
                                            <Column
                                                demandPopin="true"
                                                vAlign="Middle">
												<Text text="{i18n>VehicleList.vehicleDesc}" />
                                            </Column>
                         
                                            <Column
                                                demandPopin="true"
                                                hAlign="End"
                                                vAlign="Middle">
                                            </Column>
                                        </columns>
										 <ColumnListItem vAlign="Middle" type="Active"  press="navToAddInvolvedVehicle">

											<Label text="{INVOLVED_VEHICLE>VEHICLE_DESC}" id="descField"/>
											
											<Button
												icon="sap-icon://sys-cancel"
												type="Transparent"
												tooltip="{i18nGlobal>General.hasEdgeErrors}"
												visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
												press="removeVehiclePress" />
										</ColumnListItem>
                                    </Table>
                                </ScrollContainer>

                            </u:blocks>
                        </u:ObjectPageSubSection>
                    </u:subSections>
                </u:ObjectPageSection>
                
                <u:ObjectPageSection title="{i18n>ObjectPageSection.properties}" id="page5" >
                    <u:subSections>
                        <u:ObjectPageSubSection>
                            <u:blocks>
                                <ScrollContainer
                                    vertical="true"
                                    height="100%"
                                    width="100%">
                                    <Table
                                        noDataText="{i18n>PropertiesList.empty}"
                                        items="{INVOLVED_PROPERTY>/}"
                                        growing="true"
                                        includeItemInSelection="true"
                                        growingScrollToLoad="true">
                                        <headerToolbar>
                                            <Toolbar>
                                                <Title
                                                    id="propertyTableTitle"
                                                    text="{ path: 'INVOLVED_PROPERTY>/length', formatter: '.formatters.propertiesListTitle' }"
                                                    level="H2"/>
                                                <ToolbarSpacer></ToolbarSpacer>
                                                <Button
                                                    id="propertyAddButton"
                                                    icon="sap-icon://add"
													visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
                                                    press="createProperty" />
                                            </Toolbar>
                                        </headerToolbar>

                                        <columns>
                                            <Column
                                                demandPopin="true"
                                                vAlign="Middle">
												<Text text="{i18n>PropertiesList.propertiesDesc}" />
                                            </Column>
                         
                                            <Column
                                                demandPopin="true"
                                                hAlign="End"
                                                vAlign="Middle">
                                            </Column>
                                        </columns>
										 <ColumnListItem vAlign="Middle" type="Active"  press="navToAddInvolvedProperty">

											<Label text="{INVOLVED_PROPERTY>PROP_DESC}" id="propDescField"/>
											
											<Button
												icon="sap-icon://sys-cancel"
												type="Transparent"
												tooltip="{i18nGlobal>General.hasEdgeErrors}"
												visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}"
												press="removePropertyPress" />
										</ColumnListItem>
                                    </Table>
                                </ScrollContainer>

                            </u:blocks>
                        </u:ObjectPageSubSection>
                    </u:subSections>
                </u:ObjectPageSection>
            </u:sections>
        </u:ObjectPageLayout>
  
        <footer>
            <Bar id="otbFooter" visible="{= ${INCIDENT_MODEL>/IS_SUBMITTED} === ''}">
               <contentLeft>
                  <Button
                        id="messages-button"
                        icon="sap-icon://message-popup"
                        text="{/length}"
                        type="Emphasized"
                        press="handleMessagePopoverPress"/>
                </contentLeft>
               <contentRight>
                     <Button
                        id="submit-button"
                        type="Emphasized"
                        text="{i18n>Footer.submit}"
                        press="onSubmitConfirmationPress"/> 
                   <!--  <Button
                        id="save-draft-button"
                        type="Emphasized"
                        text="{i18n>Footer.save}"
                        press="onSubmitPress"/>
                    <Button
                        id="cancel-button"
                        text="{i18nGlobal>General.cancel}"
                        press="dep.fiori.lib.util.Utilities.navHome"/>-->
                </contentRight>
            </Bar>
        </footer>
